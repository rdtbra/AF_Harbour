 */
HB_FUNC( QT_QPRINTERINFO )
{
   if( hb_pcount() == 1 && HB_ISOBJECT( 1 ) )
   {
      HBQT_GC_T * q = ( HBQT_GC_T * ) hbqt_par_ptr( 1 );
